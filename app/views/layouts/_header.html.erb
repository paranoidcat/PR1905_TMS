            </button>
        </div>
        <!--/.navbar-header-->
        <div id="main-nav" class="collapse navbar-collapse">
            <ul class="nav navbar-nav">
                <% if current_user %>
                    <li><%= link_to 'Edit Profile',edit_user_registration_path %></li>
